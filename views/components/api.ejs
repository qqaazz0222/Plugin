<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/setting.css" />
        <link rel="stylesheet" href="/stylesheets/promotion.css" />
        <link rel="stylesheet" href="/stylesheets/api.css" />
    </head>
    <body>
        <%- include ./header%> <%- include ./promotion%>
        <div id="api-component">
            <div class="header">
                <div class="title">API 목록</div>
            </div>
            <div class="body">
                <div class="row list-wrap">
                    <div class="api-list">
                        <form class="row opt">
                            <img
                                src="/images/icon-filter.png"
                                alt=""
                                srcset=""
                            />
                            <select name="state" id="">
                                <option value="">상태</option>
                                <option value="">인기</option>
                                <option value="">신규</option>
                            </select>
                            <select name="" id="">
                                <option value="">국내 발송</option>
                                <option value="">인기</option>
                                <option value="">신규</option>
                            </select>
                            <select name="" id="">
                                <option value="">국제 발송</option>
                                <option value="">인기</option>
                                <option value="">신규</option>
                            </select>
                        </form>
                        <% for(var i = 0; i < 8; i++) { %>
                        <div class="item">
                            <div class="badge">
                                <span class="important">인기</span>
                                <span>신규</span>
                            </div>
                            <div class="title">사이트명</div>
                            <div class="link">www.site.com</div>
                            <div class="in-price">
                                <span>국내 발송</span><br />
                                <span><strong>0.0</strong> 원/건</span>
                            </div>
                            <div class="out-price">
                                <span>국제 발송(미국)</span><br />
                                <span><strong>0.000</strong> 달러/건</span>
                            </div>
                            <a class="detail" href="">자세히 보기</a>
                        </div>
                        <% } %>
                    </div>
                    <div class="sidebar">
                        <div class="title">인기 API 목록</div>
                        <div class="rank-list">
                            <% for(var i = 0; i < 10; i++){ %>
                            <div class="site">
                                <div class="rank"><%= i+1 %></div>
                                <div class="img">
                                    <img src="" alt="" srcset="" />
                                </div>
                                <div class="info">
                                    <div class="name">사이트명</div>
                                    <div class="like">0000</div>
                                </div>
                            </div>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <%- include ./footer%>
    </body>
</html>
